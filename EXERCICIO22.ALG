Algoritmo "exercicio22"


Var
   n, s: inteiro
   r : caractere
Inicio
      repita
            escreval("Digite um numero")
            leia(n)
            s <- s + n
            escreval("Quer somar mais algum numero?[S/N]")
            leia (r)
      até r="N"
      escreva("A soma dos valores é de", s)

Fimalgoritmo
